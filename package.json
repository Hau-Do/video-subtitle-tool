{
  "name": "video-timebar-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.4.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.59",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@webplayer/prismplayer-pc": "^0.4.0",
    "antd": "^4.23.5",
    "axios": "^0.27.2",
    "craco-alias": "^3.0.1",
    "craco-antd": "^2.0.0",
    "eslint": "^8.25.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.1",
    "sass": "^1.55.0",
    "tsc-files": "^1.1.3",
    "typescript": "^4.8.3",
    "web-vitals": "^2.1.4",
    "zustand": "^4.1.1"
  },
  "scripts": {
    "start": "nodemon -w craco.config.js -w ./src/theme/vars.scss -w ./src/theme/vars.less --exec \"craco start --config craco.config.js\"",
    "build": "craco build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "typescript": "tsc --project tsconfig.json --noEmit",
    "lint-staged": "lint-staged --config lint-staged.js",
    "husky-install": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.3.3",
    "@types/video.js": "^7.3.47",
    "babel-eslint": "^10.1.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.10",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "source-map-explorer": "^2.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn format"
    }
  },
  "lint-staged": {
    "*.+(js|ts|tsx|jsx)": "eslint --fix",
    "*.+(json|css|md)": "prettier --write"
  }
}

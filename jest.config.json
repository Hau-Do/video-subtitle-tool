{
  "collectCoverageFrom": [
    "src/components/**/*.{tsx,ts}",
    "!**/node_modules/**",
    "!**/src/test-utils/**",
    "!**/src/index.tsx",
    "!**/src/App.tsx"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 60,
      "functions": 60,
      "lines": 60
    }
  },
  "moduleNameMapper": {
    "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
    "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
  }
}
